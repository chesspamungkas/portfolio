<IfModule mod_rewrite.c>
    RewriteEngine On
    RewriteCond %{SERVER_PORT} 80
    RewriteRule ^(.)$ https://chesspamungkas.com/$1 [R,L] RewriteEngine on RewriteCond %{REQUEST_URI} !^/$ 
    RewriteCond %{REQUEST_FILENAME} !-d
    RewriteCond {REQUEST_FILENAME} !-f
    RewriteRule ^(.)$ / [L,QSA]
</IfModule>